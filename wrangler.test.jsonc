{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "openlist-back-work",
  "main": "src/index.ts",
  "compatibility_date": "2025-08-23",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "ENABLE_D1": true, // D1 是否启用
    "REMOTE_D1": ""    // D1 远程地址
    /* D1 Database 配置指南 ==============================
    local:// # 不填或为local:使用本地数据库：c.env.D1_DATA
    mysql://<username>:<password>@<host>:<port>/<database>
    maria://<username>:<password>@<host>:<port>/<database>
    pgsql://<username>:<password>@<host>:<port>/<database>
    =================================================== */
  },
  "kv_namespaces": [
    {
      "binding": "KV_DATA",
      "id": "46e4578c62c548adb2b598a1f532e0ba"
    }
  ],
  "d1_databases": [
    {
      "binding": "D1_DATA",
      "database_name": "openlist_data",
      "database_id": "3c7aa49c-d468-4f1c-b3a1-095a6039baf3"
    }
  ]
  // "observability": {
  //   "enabled": true,
  //   "head_sampling_rate": 1
  // }
}